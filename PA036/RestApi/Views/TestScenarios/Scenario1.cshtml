@using System.Web.Optimization
@model dynamic

@{
    ViewBag.Title = "ExampleScenario1";
    Layout = "~/Views/Shared/_AppLayout.cshtml";
}

<h2>ExampleScenario1</h2>

<div class="">
    <p class="lead">Hey!!! This is the ExampleScenario1. To execute test and see the results, press the button:</p>

    <button id="scenario1-execute" class="btn btn-info pull-right" data-url="@Url.RouteUrl("DefaultApi", new {controller = "Scenario1", httproute = true})">
        Run test
    </button>
</div>

<div id="container" style="min-width: 310px; height: 400px; margin: 0 auto"></div>

@section scripts{
    @Scripts.Render("~/Scripts/pages/testScenarios/scenario1.js")
}